msgstr "büyü;sekiz;top;8;kristal;🔮;orta;🎱;"

#: src/Content.vala:30
msgid "It is certain."
msgstr "Bu kesin."

#: src/Content.vala:38
msgid "Without a doubt."
msgstr "Şüphesiz."

#: src/Content.vala:42
msgid "Yes—definitely."
msgstr "Dostum Kesinlikle"

#: src/Content.vala:46
msgid "You may rely on it."
msgstr "Ona Güvenebilirsiniz"

#: src/Content.vala:50
msgid "As I see it, yes."
msgstr "Gördüğüm kadarıyla evet."

#: src/Content.vala:54
msgid "Most likely."
msgstr "Azdan az çoktan çok gider."

#: src/Content.vala:58
msgid "Outlook good."
msgstr "İyi görünüyor"

#: src/Content.vala:62
msgid "Yes."
msgstr "Evet"

#: src/Content.vala:66
msgid "Signs point to yes."
msgstr "İşaretler evet'i gösteriyor."

#: src/Content.vala:70
msgid "Don't count on it."
msgstr "Ona güvenme."

#: src/Content.vala:74
msgid "My reply is no."
msgstr "Cevabım hayır."

#: src/Content.vala:78
msgid "My sources say no."
msgstr "Kaynaklarım hayır diyor."

#: src/Content.vala:82
msgid "Outlook not so good."
msgstr "Görünüm pek iyi değil."

#: src/Content.vala:86
msgid "Very doubtful."
msgstr "Çok şüpheli."

#: src/Content.vala:90
msgid "Reply hazy, try again."
msgstr "Bulanık cevap tekrar deneyin."

#: src/Content.vala:94
msgid "Ask again later."
msgstr "Sonra tekrar sor."

#: src/Content.vala:98
msgid "Better not tell you now."
msgstr "Sana şimdi söylememek daha iyi.(Aga be)"

#: src/Content.vala:102
msgid "Cannot predict now."
msgstr "Şimdi tahmin edemem."

#: src/Content.vala:106
msgid "Concentrate and ask again."
msgstr "Konsantre ol ve tekrar sor."

#: src/MainWindow.vala:46
